{
  "name": "hubspot-integration",
  "version": "1.0.0",
  "description": "API with a HubSpot integration using a Postgres database and webhooks",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "start:webhook": "yarn build && node dist/webhookServer.js",
    "lint": "eslint --ext .js,.ts src/**/*",
    "lint:fix": "eslint --fix --ext .js,.ts src/**/*",
    "db:start": "docker-compose up",
    "db:stop": "docker-compose down",
    "test": "jest --runInBand",
    "test:clear": "jest --clearCache",
    "expose": "ngrok http 3000"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.6",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-define-config": "^2.1.0",
    "jest": "^29.7.0",
    "ngrok": "^5.0.0-beta.2",
    "nock": "^14.0.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
